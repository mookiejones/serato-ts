{
  "name": "serato-ts",
  "version": "3.2.6",
  "description": "Manage Serato Crates programatically via NodeJS. Direct Port of https://github.com/mookiejones/serato-ts.git",
  "main": "dist/index.js",
  "scripts": {
    "format:check": "prettier --list-different .",
    "format": "prettier --write .",
    "clean": "rimraf ./dist && rimraf ./coverage",
    "build": "npx tsc",
    "test": "jest",
    "test:ci": "npm run test",
    "testCoverage": "jest --coverage",
    "testCache": "jest --no-cache",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run clean && npm run format && npm run build && npm run package && npm test && npm pub && npm pulish",
    "pub": "npm version patch --force && npm publish --access=public",
    "version": "node --version && npm --version && tsc --version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mookiejones/serato-ts.git"
  },
  "keywords": [
    "serato",
    "javascript",
    "music",
    "crates",
    "dj"
  ],
  "author": "Charles Berman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mookiejones/serato-ts/issues"
  },
  "homepage": "https://github.com/mookiejones/serato-ts#readme",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.5",
    "@types/node": "^20.14.2",
    "@typescript-eslint/parser": "^7.13.0",
    "@vercel/ncc": "^0.38.1",
    "binary-reader": "^0.1.2",
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "prettier": "3.3.2",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
